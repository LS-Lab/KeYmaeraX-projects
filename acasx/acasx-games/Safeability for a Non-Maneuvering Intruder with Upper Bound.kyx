Theorem "ACAS X Hybrid Games/Safeable Non Case 2"

ProgramVariables
  /* horizontal */
  Real r;    /* relative distance in ft; xi - xo */
             /* x = r */
  Real rv;   /* relative speed -(vi - vo) */
  
  /* vertical */
  Real h;    /* relative altitude in ft */
             /* if negative: the intruder is lower than the ownship */
             /* h := hi - ho */
             /* z = h */
  Real v;    /* vertical velocity of ownship */
  Real vlo;  /* target minimum vertical velocity */
  Real vup;  /* target maximum vertical velocity */
  Real w;    /* velocity bound */
  Real ao;    /* ownship acceleration */
  
  Real tt;   /* clock */
End.

Problem
  loopInv(r,h,v,w,vlo,vup,rv,tt)&w*vlo<=w*v&w*v<w*vup->\exists ao ((-amax()<=ao&ao<=amax())&[motion{|^@|};]loopInv(r,h,v,w,vlo,vup,rv,tt))
End.

Tactic "ACAS X Hybrid Games/Proof Safeable Non Case 2"
implyR('R=="loopInv(r,h,v,w,vlo,vup,rv,tt)&w*vlo<=w*v&w*v < w*vup->\exists ao ((-amax()<=ao&ao<=amax())&[motion{|^@|};]loopInv(r,h,v,w,vlo,vup,rv,tt))");
andL('L=="loopInv(r,h,v,w,vlo,vup,rv,tt)&w*vlo<=w*v&w*v < w*vup");
existsR("0", 'R=="\exists ao ((-amax()<=ao&ao<=amax())&[motion{|^@|};]loopInv(r,h,v,w,vlo,vup,rv,tt))");
