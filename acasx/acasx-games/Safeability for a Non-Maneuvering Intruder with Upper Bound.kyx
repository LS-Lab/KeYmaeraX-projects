        unfold;
        allL("t_", 'L=="\forall s_ (0<=s_&s_<=t_->s_+tt<=epsilon()&(w*ao<=0|w*(ao*s_+v)<=w*vup))");
        simplify('L=="0<=t_&t_<=t_->t_+tt<=epsilon()&(w*ao<=0|w*(ao*t_+v)<=w*vup)");
