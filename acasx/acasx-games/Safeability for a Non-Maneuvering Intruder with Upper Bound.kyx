        andL('L)*;
        simplify('R=="0<=t_+tt&t_+tt<=epsilon()&(w=wUp()|w=wLo())&init(w,rv)&CimplSafe((-rv)*t_+r,-(ao*(t_^2/2)+v*t_)+h,ao*t_+v,w,vlo,vup,rv,epsilon()-(t_+tt))");
        andR('R=="0<=t_+tt&CimplSafe((-rv)*t_+r,-(ao*(t_^2/2)+v*t_)+h,ao*t_+v,w,vlo,vup,rv,epsilon()-(t_+tt))"); <(
          "0<=t_+tt":
            QE,
