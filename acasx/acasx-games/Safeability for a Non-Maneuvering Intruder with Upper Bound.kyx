Theorem "ACAS X Hybrid Games/Safeable Non"

ProgramVariables
  /* horizontal */
  Real r;    /* relative distance in ft; xi - xo */
             /* x = r */
  Real rv;   /* relative speed -(vi - vo) */
  
  /* vertical */
  Real h;    /* relative altitude in ft */
             /* if negative: the intruder is lower than the ownship */
             /* h := hi - ho */
             /* z = h */
  Real v;    /* vertical velocity of ownship */
  Real vlo;  /* target minimum vertical velocity */
  Real vup;  /* target maximum vertical velocity */
  Real w;    /* velocity bound */
  Real ao;    /* ownship acceleration */
  
  Real tt;   /* clock */
End.

Problem
    init(w,rv)
  & CimplSafe(r,h,v,w,vlo,vup,rv,epsilon)
  & tt=0
  ->
  [{
     { { w:=wUp; ++ w:=wLo; }
       vlo := *; 
       vup := *;        
       ?CimplSafe(r,h,v,w,vlo,vup,rv,epsilon);
     }
     tt := 0;
     {
      { ao := *; ?(-amax<=ao & ao<=amax); }^@;
      motion;
     }*@invariant(loopInv(r,h,v,w,vlo,vup,rv,tt))
   }*@invariant(loopInv(r,h,v,w,vlo,vup,rv,tt))
  ](abs(r) > rp | abs(h) > hp)
End.

Tactic "ACAS X Hybrid Games/Proof Safeable Non"
implyR('R=="init(w,rv)&CimplSafe(r,h,v,w,vlo,vup,rv,epsilon())&tt=0->[{{{w:=wUp();++w:=wLo();}vlo:=*;vup:=*;?CimplSafe(r,h,v,w,vlo,vup,rv,epsilon());}tt:=0;{{ao:=*;?-amax()<=ao&ao<=amax();}^@motion{|^@|};}*}*](abs(r)>rp()|abs(h)>hp())");
loopRule("loopInv(r,h,v,w,vlo,vup,rv,tt)", 'R=="[{{{w:=wUp();++w:=wLo();}vlo:=*;vup:=*;?CimplSafe(r,h,v,w,vlo,vup,rv,epsilon());}tt:=0;{{ao:=*;?-amax()<=ao&ao<=amax();}^@motion{|^@|};}*}*](abs(r)>rp()|abs(h)>hp())"); <(
  "Init":
    expand "loopInv" ; expand "init" ; andL('L)* ; allL2R('L=="tt=0") ; simplify('R=="0<=0&0<=epsilon()&(w=wUp()|w=wLo())&(rp()>=0&hp()>0&rv>=0&alo()>0&(w=wUp()|w=wLo())&aup()>alo()&amax()>=alo()&epsilon()>=0)&CimplSafe(r,h,v,w,vlo,vup,rv,epsilon()-0)") ; closeT,
  "Step":
    composeb('R=="[{{w:=wUp();++w:=wLo();}vlo:=*;vup:=*;?CimplSafe(r,h,v,w,vlo,vup,rv,epsilon());}tt:=0;{{ao:=*;?-amax()<=ao&ao<=amax();}^@motion{|^@|};}*]loopInv(r,h,v,w,vlo,vup,rv,tt)");
    MR("loopInv(r,h,v,w,vlo,vup,rv,0)", 'R=="[{w:=wUp();++w:=wLo();}vlo:=*;vup:=*;?CimplSafe(r,h,v,w,vlo,vup,rv,epsilon());][tt:=0;{{ao:=*;?-amax()<=ao&ao<=amax();}^@motion{|^@|};}*]loopInv(r,h,v,w,vlo,vup,rv,tt)"); <(
      "Use Q->P":
        unfold; <(
          "[w:=wUp();][vlo:=*;vup:=*;?CimplSafe(r,h,v,w,vlo,vup,rv,epsilon());]loopInv(r,h,v,w,vlo,vup,rv,0)":
            expand "loopInv";
            expand "init";
            andL('L)*;
            simplify('R=="0<=0&0<=epsilon()&(wUp()=wUp()|wUp()=wLo())&(rp()>=0&hp()>0&rv>=0&alo()>0&(wUp()=wUp()|wUp()=wLo())&aup()>alo()&amax()>=alo()&epsilon()>=0)&CimplSafe(r,h,v,wUp(),vlo,vup,rv,epsilon()-0)");
            closeT,
          "[w:=wLo();][vlo:=*;vup:=*;?CimplSafe(r,h,v,w,vlo,vup,rv,epsilon());]loopInv(r,h,v,w,vlo,vup,rv,0)":
            expand "loopInv";
            expand "init";
            andL('L)*;
            simplify('R=="0<=0&0<=epsilon()&(wLo()=wUp()|wLo()=wLo())&(rp()>=0&hp()>0&rv>=0&alo()>0&(wLo()=wUp()|wLo()=wLo())&aup()>alo()&amax()>=alo()&epsilon()>=0)&CimplSafe(r,h,v,wLo(),vlo,vup,rv,epsilon()-0)");
            closeT
        ),
      "Show [a]Q":
        unfold;
        loopRule("loopInv(r,h,v,w,vlo,vup,rv,tt)", 'R=="[{{ao:=*;?-amax()<=ao&ao<=amax();}^@motion{|^@|};}*]loopInv(r,h,v,w,vlo,vup,rv,tt)"); <(
          "Init":
            allL2R('L=="tt=0");
            id,
          "Step":
            chaseAt('R=="[{ao:=*;?-amax()<=ao&ao<=amax();}^@motion{|^@|};]loopInv(r,h,v,w,vlo,vup,rv,tt)");
            cut("w*v < w*vlo|w*vlo<=w*v&w*v < w*vup|w*vup<=w*v"); <(
              "Use":
                orL('L=="w*v < w*vlo|w*vlo<=w*v&w*v < w*vup|w*vup<=w*v"); <(
                  "w*v < w*vlo":
                    useLemma("ACAS X Hybrid Games/Safeable Non Case 1", "prop"),
                  "w*vlo<=w*v&w*v < w*vup|w*vup<=w*v":
                    orL('L=="w*vlo<=w*v&w*v < w*vup|w*vup<=w*v"); <(
                      "w*vlo<=w*v&w*v < w*vup":
                        useLemma("ACAS X Hybrid Games/Safeable Non Case 2", "prop"),
                      "w*vup<=w*v":
                        useLemma("ACAS X Hybrid Games/Safeable Non Case 3", "prop")
                    )
                ),
              "Show":
                hideR('R=="\exists ao ((-amax()<=ao&ao<=amax())&[motion{|^@|};]loopInv(r,h,v,w,vlo,vup,rv,tt))");
                expand "loopInv";
                expand "init";
                andL('L)*;
                QE using "0<=tt :: tt<=epsilon() :: w=wUp()|w=wLo() :: rp()>=0 :: hp()>0 :: rv>=0 :: alo()>0 :: w=wUp()|w=wLo() :: aup()>alo() :: amax()>=alo() :: epsilon()>=0 :: w*v < w*vlo|w*vlo<=w*v&w*v < w*vup|w*vup<=w*v :: nil"
            ),
          "Post":
            id
        )
    ),
  "Post":
    expand "loopInv";
    expand "CimplSafe";
    andL('L)*;
    orL('L=="LimplSafe(r,h,v,w,vlo,rv,epsilon()-tt)|UimplSafe(r,h,v,w,vup,rv,epsilon()-tt)"); <(
      "LimplSafe(r,h,v,w,vlo,rv,epsilon()-tt)":
        expand "LimplSafe";
        andL('L=="LimplEp(r,h,v,w,vlo,rv,epsilon()-tt)&\forall hexL \forall vexL (0<=epsilon()-tt&epsilon()-tt < Tlo(v,w,vlo)&hexL=w*alo()/2*(epsilon()-tt)^2+v*(epsilon()-tt)&vexL=w*alo()*(epsilon()-tt)+v|epsilon()-tt>=Tlo(v,w,vlo)&hexL=vlo*(epsilon()-tt)-w*maxI(v,w,vlo)^2/(2*alo())&vexL=vlo->\exists vexLo Limpl(w,vexLo,vexL,r-rv*(epsilon()-tt),rv,h-hexL))");
        hideL('L=="\forall hexL \forall vexL (0<=epsilon()-tt&epsilon()-tt < Tlo(v,w,vlo)&hexL=w*alo()/2*(epsilon()-tt)^2+v*(epsilon()-tt)&vexL=w*alo()*(epsilon()-tt)+v|epsilon()-tt>=Tlo(v,w,vlo)&hexL=vlo*(epsilon()-tt)-w*maxI(v,w,vlo)^2/(2*alo())&vexL=vlo->\exists vexLo Limpl(w,vexLo,vexL,r-rv*(epsilon()-tt),rv,h-hexL))");
        expand "LimplEp";
        allL("0", 'L=="\forall t \forall hn (t<=epsilon()-tt&Alo(v,w,vlo,hn,t)->abs(r-rv*t)>rp()|w*(hn-h)>hp())");
        allL("0", 'L=="\forall hn (0<=epsilon()-tt&Alo(v,w,vlo,hn,0)->abs(r-rv*0)>rp()|w*(hn-h)>hp())");
        QE,
      "UimplSafe(r,h,v,w,vup,rv,epsilon()-tt)":
        expand "UimplSafe";
        andL('L=="UimplEp(r,h,v,w,vup,rv,epsilon()-tt)&\forall hexU \forall vexU (0<=epsilon()-tt&epsilon()-tt < Tup(v,w,vup)&hexU=w*aup()/2*(epsilon()-tt)^2+v*(epsilon()-tt)&vexU=w*aup()*(epsilon()-tt)+v|epsilon()-tt>=Tup(v,w,vup)&hexU=w*max(w*vup,w*v)*(epsilon()-tt)-w*maxI(v,w,vup)^2/(2*aup())&vexU=w*max(w*vup,w*v)->\exists vexUp Limpl(-w,vexUp,vexU,r-rv*(epsilon()-tt),rv,h-hexU))");
        hideL('L=="\forall hexU \forall vexU (0<=epsilon()-tt&epsilon()-tt < Tup(v,w,vup)&hexU=w*aup()/2*(epsilon()-tt)^2+v*(epsilon()-tt)&vexU=w*aup()*(epsilon()-tt)+v|epsilon()-tt>=Tup(v,w,vup)&hexU=w*max(w*vup,w*v)*(epsilon()-tt)-w*maxI(v,w,vup)^2/(2*aup())&vexU=w*max(w*vup,w*v)->\exists vexUp Limpl(-w,vexUp,vexU,r-rv*(epsilon()-tt),rv,h-hexU))");
        expand "UimplEp";
        allL("0", 'L=="\forall t \forall hn (t<=epsilon()-tt&Aup(v,w,vup,hn,t)->abs(r-rv*t)>rp()|w*(h-hn)>hp())");
        allL("0", 'L=="\forall hn (0<=epsilon()-tt&Aup(v,w,vup,hn,0)->abs(r-rv*0)>rp()|w*(h-hn)>hp())");
        QE
    )
)
End.

