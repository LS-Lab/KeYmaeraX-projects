                                                        hideL('L=="(-w)*(vexUp*t-(-w)*maxI(w*max(w*vup,w*v),-w,vexUp)^2/(2*alo())-(h-(w*max(w*vup,w*v)*(epsilon()-tt)-w*maxI(v,w,vup)^2/(2*aup()))))>hp()");
