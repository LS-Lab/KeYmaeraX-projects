  "-amax()<=ao&ao<=amax()":
    expand "loopInv";
    expand "init";
    andL('L)*;
    QE using "ao=0 :: 0<=tt :: w*vlo<=w*v :: w*v < w*vup :: tt<=epsilon() :: w=wUp()|w=wLo() :: rp()>=0 :: hp()>0 :: rv>=0 :: alo()>0 :: w=wUp()|w=wLo() :: aup()>alo() :: amax()>=alo() :: epsilon()>=0 :: -amax()<=ao&ao<=amax() :: nil",
  "[motion{|^@|};]loopInv(r,h,v,w,vlo,vup,rv,tt)":
    expand "motion";
