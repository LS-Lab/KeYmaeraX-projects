
SharedDefinitions
  Real hp;
  Real rp;
  Real alo;
  Real aup;
  Real amax; /* maximal aircraft capability: vertical acceleration. */

  Real wUp = -1;
  Real wLo = 1;
  Real epsilon;

  Real maxI(Real v, Real w, Real vref) = max(0, w*(vref-v));
  Real Tlo(Real v, Real w, Real vlo) = maxI(v,w,vlo)/alo;
  Real Tup(Real v, Real w, Real vup) = maxI(v,w,vup)/aup;

  Bool Alo(Real v, Real w, Real vlo, Real hn, Real t) <-> (
      (0 <= t & t < Tlo(v,w,vlo) & hn = w*alo/2*t^2 + v*t)
    | (t >= Tlo(v,w,vlo)         & hn = vlo*t - w*maxI(v,w,vlo)^2/(2*alo))
  );

  Bool Aup(Real v, Real w, Real vup, Real hn, Real t) <-> (
      (0 <= t & t < Tup(v,w,vup) & hn = w*aup/2*t^2 + v*t)
    | (t >= Tup(v,w,vup)         & hn = w*max(w*vup,w*v)*t - w*maxI(v,w,vup)^2/(2*aup))
  );

  Bool Limpl(Real w, Real vlo, Real v, Real r, Real rv, Real h) <-> (
    \forall t \forall hn (
        Alo(v,w,vlo,hn,t) -> (abs(r - rv*t) > rp | w*(hn-h) > hp)
    )
  ).

  Bool LimplEp(Real r, Real h, Real v, Real w, Real vlo, Real rv, Real ep) <->
    \forall t \forall hn (
        (t<=ep) & Alo(v,w,vlo,hn,t) -> (abs(r - rv*t) > rp | w*(hn-h) > hp)
    );

  Bool UimplEp(Real r, Real h, Real v, Real w, Real vup, Real rv, Real ep) <->
    \forall t \forall hn (
        (t<=ep) & Aup(v,w,vup,hn,t) -> (abs(r - rv*t) > rp | w*(h-hn) > hp)
    );

  Bool LimplSafe(Real r, Real h, Real v, Real w, Real vlo, Real rv, Real ep) <-> (
      LimplEp(r,h,v,w,vlo,rv,ep)
    & \forall hexL \forall vexL (
         (0 <= ep & ep < Tlo(v,w,vlo) & hexL = (w*alo)/2*ep^2+v*ep & vexL = w*alo*ep + v)
      |  (ep >= Tlo(v,w,vlo)          & hexL = vlo*ep - (w*maxI(v,w,vlo)^2)/(2*alo) & vexL = vlo)
      -> \exists vexLo (Limpl(w,vexLo,vexL,r - rv*ep, rv, h - hexL))
      )
    );
  
  Bool UimplSafe(Real r, Real h, Real v, Real w, Real vup, Real rv, Real ep)<-> (
      UimplEp(r,h,v,w,vup,rv,ep)
    & \forall hexU \forall vexU (
          (0 <= ep & ep < Tup(v,w,vup)  & hexU = (w*aup/2)*ep^2 + v*ep                           & vexU = w*aup*ep + v)
        | (ep >= Tup(v,w,vup)           & hexU = w*max(w*vup,w*v)*ep - w*maxI(v,w,vup)^2/(2*aup) & vexU = w*max(w*vup,w*v))
        -> \exists vexUp (Limpl(-w,vexUp,vexU,r - rv*ep, rv, h - hexU))
        )
    );
 
  Bool CimplSafe(Real r, Real h, Real v, Real w, Real vlo, Real vup, Real rv, Real ep) <-> 
    w*vlo <= w*vup & (LimplSafe(r,h,v,w,vlo,rv,ep) | UimplSafe(r,h,v,w,vup,rv,ep));

  Bool init(Real w, Real rv) <-> rp>=0 & hp>0 & rv>=0 & alo>0 & (w=wUp | w=wLo) & aup>alo & amax>=alo & epsilon >= 0;

  Bool loopInv(Real r, Real h, Real v, Real w, Real vlo, Real vup, Real rv, Real tt) <-> 
    0 <= tt & tt <= epsilon & (w=wUp | w=wLo) & init(w,rv) & CimplSafe(r,h,v,w,vlo,vup,rv,epsilon-tt);

  HP inMotion ::= { {r' = -rv, h' = -v, v' = ao, tt'=1 & (tt <= epsilon & (w*ao<=0 | w*v<=w*vup)) } };

  HP inEvent ::= { {r' = -rv, h' = -v, v' = ao, tt'=1 & (tt <= epsilon & (w*ao>=0 & w*v>=w*vup)) } };
  
  HP motion ::= { inMotion; ++ inEvent; };
End.


Theorem "ACAS X Hybrid Games/Safeable Non Case 1"

ProgramVariables
  /* horizontal */
  Real r;    /* relative distance in ft; xi - xo */
             /* x = r */
  Real rv;   /* relative speed -(vi - vo) */
  
  /* vertical */
  Real h;    /* relative altitude in ft */
             /* if negative: the intruder is lower than the ownship */
             /* h := hi - ho */
             /* z = h */
  Real v;    /* vertical velocity of ownship */
  Real vlo;  /* target minimum vertical velocity */
  Real vup;  /* target maximum vertical velocity */
  Real w;    /* velocity bound */
  Real ao;    /* ownship acceleration */
  
  Real tt;   /* clock */
End.

Problem
  loopInv(r,h,v,w,vlo,vup,rv,tt)&w*v < w*vlo->\exists ao ((-amax()<=ao&ao<=amax())&[motion{|^@|};]loopInv(r,h,v,w,vlo,vup,rv,tt))
End.

Tactic "ACAS X Hybrid Games/Proof Safeable Non Case 1"
implyR('R=="loopInv(r,h,v,w,vlo,vup,rv,tt)&w*v < w*vlo->\exists ao ((-amax()<=ao&ao<=amax())&[motion{|^@|};]loopInv(r,h,v,w,vlo,vup,rv,tt))");
andL('L=="loopInv(r,h,v,w,vlo,vup,rv,tt)&w*v < w*vlo");
existsR("w*alo()", 'R=="\exists ao ((-amax()<=ao&ao<=amax())&[motion{|^@|};]loopInv(r,h,v,w,vlo,vup,rv,tt))");
andR('R=="(-amax()<=ao&ao<=amax())&[motion{|^@|};]loopInv(r,h,v,w,vlo,vup,rv,tt)"); <(
  "-amax()<=ao&ao<=amax()":
    expand "loopInv" ; expand "init" ; andL('L)* ; hideL('L=="CimplSafe(r,h,v,w,vlo,vup,rv,epsilon()-tt)") ; QE,
  "[motion{|^@|};]loopInv(r,h,v,w,vlo,vup,rv,tt)":
    expand "motion" ; unfold; <(
      "[inMotion{|^@|};]loopInv(r,h,v,w,vlo,vup,rv,tt)":
        expand "inMotion" ; solve('R=="[{r'=-rv,h'=-v,v'=ao,tt'=1&tt<=epsilon()&(w*ao<=0|w*v<=w*vup)}]loopInv(r,h,v,w,vlo,vup,rv,tt)") ; unfold ; allL("t_", 'L=="\forall s_ (0<=s_&s_<=t_->s_+tt<=epsilon()&(w*ao<=0|w*(ao*s_+v)<=w*vup))") ; simplify('L=="0<=t_&t_<=t_->t_+tt<=epsilon()&(w*ao<=0|w*(ao*t_+v)<=w*vup)") ; expand "loopInv" ; expand "init" ; andL('L)* ; simplify('R=="0<=t_+tt&t_+tt<=epsilon()&(w=wUp()|w=wLo())&(rp()>=0&hp()>0&rv>=0&alo()>0&(w=wUp()|w=wLo())&aup()>alo()&amax()>=alo()&epsilon()>=0)&CimplSafe((-rv)*t_+r,-(ao*(t_^2/2)+v*t_)+h,ao*t_+v,w,vlo,vup,rv,epsilon()-(t_+tt))") ; andR('R=="0<=t_+tt&CimplSafe((-rv)*t_+r,-(ao*(t_^2/2)+v*t_)+h,ao*t_+v,w,vlo,vup,rv,epsilon()-(t_+tt))"); <(
          "0<=t_+tt":
            QE,
          "CimplSafe((-rv)*t_+r,-(ao*(t_^2/2)+v*t_)+h,ao*t_+v,w,vlo,vup,rv,epsilon()-(t_+tt))":
            expand "CimplSafe";
            andL('L=="w*vlo<=w*vup&(LimplSafe(r,h,v,w,vlo,rv,epsilon()-tt)|UimplSafe(r,h,v,w,vup,rv,epsilon()-tt))");
            simplify('R=="w*vlo<=w*vup&(LimplSafe((-rv)*t_+r,-(ao*(t_^2/2)+v*t_)+h,ao*t_+v,w,vlo,rv,epsilon()-(t_+tt))|UimplSafe((-rv)*t_+r,-(ao*(t_^2/2)+v*t_)+h,ao*t_+v,w,vup,rv,epsilon()-(t_+tt)))");
            orR('R=="LimplSafe((-rv)*t_+r,-(ao*(t_^2/2)+v*t_)+h,ao*t_+v,w,vlo,rv,epsilon()-(t_+tt))|UimplSafe((-rv)*t_+r,-(ao*(t_^2/2)+v*t_)+h,ao*t_+v,w,vup,rv,epsilon()-(t_+tt))");
            orL('L=="LimplSafe(r,h,v,w,vlo,rv,epsilon()-tt)|UimplSafe(r,h,v,w,vup,rv,epsilon()-tt)"); <(
              "LimplSafe(r,h,v,w,vlo,rv,epsilon()-tt)":
                hideR('R=="UimplSafe((-rv)*t_+r,-(ao*(t_^2/2)+v*t_)+h,ao*t_+v,w,vup,rv,epsilon()-(t_+tt))");
                expand "LimplSafe" ; andL('L=="LimplEp(r,h,v,w,vlo,rv,epsilon()-tt)&\forall hexL \forall vexL (0<=epsilon()-tt&epsilon()-tt < Tlo(v,w,vlo)&hexL=w*alo()/2*(epsilon()-tt)^2+v*(epsilon()-tt)&vexL=w*alo()*(epsilon()-tt)+v|epsilon()-tt>=Tlo(v,w,vlo)&hexL=vlo*(epsilon()-tt)-w*maxI(v,w,vlo)^2/(2*alo())&vexL=vlo->\exists vexLo Limpl(w,vexLo,vexL,r-rv*(epsilon()-tt),rv,h-hexL))") ; andR('R=="LimplEp((-rv)*t_+r,-(ao*(t_^2/2)+v*t_)+h,ao*t_+v,w,vlo,rv,epsilon()-(t_+tt))&\forall hexL \forall vexL (0<=epsilon()-(t_+tt)&epsilon()-(t_+tt) < Tlo(ao*t_+v,w,vlo)&hexL=w*alo()/2*(epsilon()-(t_+tt))^2+(ao*t_+v)*(epsilon()-(t_+tt))&vexL=w*alo()*(epsilon()-(t_+tt))+(ao*t_+v)|epsilon()-(t_+tt)>=Tlo(ao*t_+v,w,vlo)&hexL=vlo*(epsilon()-(t_+tt))-w*maxI(ao*t_+v,w,vlo)^2/(2*alo())&vexL=vlo->\exists vexLo Limpl(w,vexLo,vexL,(-rv)*t_+r-rv*(epsilon()-(t_+tt)),rv,-(ao*(t_^2/2)+v*t_)+h-hexL))") ; <(
          "LimplEp((-rv)*t_+r,-(ao*(t_^2/2)+v*t_)+h,ao*t_+v,w,vlo,rv,epsilon()-(t_+tt))": hideL('L=="\forall hexL \forall vexL (0<=epsilon()-tt&epsilon()-tt < Tlo(v,w,vlo)&hexL=w*alo()/2*(epsilon()-tt)^2+v*(epsilon()-tt)&vexL=w*alo()*(epsilon()-tt)+v|epsilon()-tt>=Tlo(v,w,vlo)&hexL=vlo*(epsilon()-tt)-w*maxI(v,w,vlo)^2/(2*alo())&vexL=vlo->\exists vexLo Limpl(w,vexLo,vexL,r-rv*(epsilon()-tt),rv,h-hexL))") ; expand "LimplEp" ; allR('R=="\forall t \forall hn (t<=epsilon()-(t_+tt)&Alo(ao*t_+v,w,vlo,hn,t)->abs((-rv)*t_+r-rv*t)>rp()|w*(hn-(-(ao*(t_^2/2)+v*t_)+h))>hp())") ; allR('R=="\forall hn (t<=epsilon()-(t_+tt)&Alo(ao*t_+v,w,vlo,hn,t)->abs((-rv)*t_+r-rv*t)>rp()|w*(hn-(-(ao*(t_^2/2)+v*t_)+h))>hp())") ; allL("t+t_", 'L=="\forall t \forall hn (t<=epsilon()-tt&Alo(v,w,vlo,hn,t)->abs(r-rv*t)>rp()|w*(hn-h)>hp())") ; hideL('L=="w=wUp()|w=wLo()") ; expand "Alo" ; cut("t+t_<=epsilon()-tt&(0<=t+t_&t+t_ < Tlo(v,w,vlo)|t+t_>=Tlo(v,w,vlo))") ; <(
              "Use": andL('L=="t+t_<=epsilon()-tt&(0<=t+t_&t+t_ < Tlo(v,w,vlo)|t+t_>=Tlo(v,w,vlo))") ; simplify('L=="\forall hn (t+t_<=epsilon()-tt&(0<=t+t_&t+t_ < Tlo(v,w,vlo)&hn=w*alo()/2*(t+t_)^2+v*(t+t_)|t+t_>=Tlo(v,w,vlo)&hn=vlo*(t+t_)-w*maxI(v,w,vlo)^2/(2*alo()))->abs(r-rv*(t+t_))>rp()|w*(hn-h)>hp())") using "\forall hn (t+t_<=epsilon()-tt&(0<=t+t_&t+t_ < Tlo(v,w,vlo)&hn=w*alo()/2*(t+t_)^2+v*(t+t_)|t+t_>=Tlo(v,w,vlo)&hn=vlo*(t+t_)-w*maxI(v,w,vlo)^2/(2*alo()))->abs(r-rv*(t+t_))>rp()|w*(hn-h)>hp()) :: t+t_<=epsilon()-tt :: 0<=t+t_&t+t_ < Tlo(v,w,vlo)|t+t_>=Tlo(v,w,vlo) :: t<=epsilon()-(t_+tt)&(0<=t&t < Tlo(ao*t_+v,w,vlo)&hn=w*alo()/2*t^2+(ao*t_+v)*t|t>=Tlo(ao*t_+v,w,vlo)&hn=vlo*t-w*maxI(ao*t_+v,w,vlo)^2/(2*alo()))->abs((-rv)*t_+r-rv*t)>rp()|w*(hn-(-(ao*(t_^2/2)+v*t_)+h))>hp() :: nil" ; orL('L=="0<=t+t_&t+t_ < Tlo(v,w,vlo)|t+t_>=Tlo(v,w,vlo)") ; <(
                  "0<=t+t_&t+t_ < Tlo(v,w,vlo)": allL("w*alo()/2*(t+t_)^2+v*(t+t_)", 'L=="\forall hn (0<=t+t_&t+t_ < Tlo(v,w,vlo)&hn=w*alo()/2*(t+t_)^2+v*(t+t_)|t+t_>=Tlo(v,w,vlo)&hn=vlo*(t+t_)-w*maxI(v,w,vlo)^2/(2*alo())->abs(r-rv*(t+t_))>rp()|w*(hn-h)>hp())") ; simplify('L=="0<=t+t_&t+t_ < Tlo(v,w,vlo)&w*alo()/2*(t+t_)^2+v*(t+t_)=w*alo()/2*(t+t_)^2+v*(t+t_)|t+t_>=Tlo(v,w,vlo)&w*alo()/2*(t+t_)^2+v*(t+t_)=vlo*(t+t_)-w*maxI(v,w,vlo)^2/(2*alo())->abs(r-rv*(t+t_))>rp()|w*(w*alo()/2*(t+t_)^2+v*(t+t_)-h)>hp()") ; orL('L=="abs(r-rv*(t+t_))>rp()|w*(w*alo()/2*(t+t_)^2+v*(t+t_)-h)>hp()") ; <(
                      "abs(r-rv*(t+t_))>rp()": QE,
                      "w*(w*alo()/2*(t+t_)^2+v*(t+t_)-h)>hp()": expandAllDefs ; implyR('R=="t<=epsilon()-(t_+tt)&(0<=t&t < max(0,w*(vlo-(ao*t_+v)))/alo()&hn=w*alo()/2*t^2+(ao*t_+v)*t|t>=max(0,w*(vlo-(ao*t_+v)))/alo()&hn=vlo*t-w*max(0,w*(vlo-(ao*t_+v)))^2/(2*alo()))->abs((-rv)*t_+r-rv*t)>rp()|w*(hn-(-(ao*(t_^2/2)+v*t_)+h))>hp()") ; orL('L=="w=(-1)|w=1") ; <(
                          "w=(-1)": andL('L=="t<=epsilon()-(t_+tt)&(0<=t&t < max(0,w*(vlo-(ao*t_+v)))/alo()&hn=w*alo()/2*t^2+(ao*t_+v)*t|t>=max(0,w*(vlo-(ao*t_+v)))/alo()&hn=vlo*t-w*max(0,w*(vlo-(ao*t_+v)))^2/(2*alo()))") ; orL('L=="0<=t&t < max(0,w*(vlo-(ao*t_+v)))/alo()&hn=w*alo()/2*t^2+(ao*t_+v)*t|t>=max(0,w*(vlo-(ao*t_+v)))/alo()&hn=vlo*t-w*max(0,w*(vlo-(ao*t_+v)))^2/(2*alo())") ; <(
                              "0<=t&t < max(0,w*(vlo-(ao*t_+v)))/alo()&hn=w*alo()/2*t^2+(ao*t_+v)*t": QE,
                              "t>=max(0,w*(vlo-(ao*t_+v)))/alo()&hn=vlo*t-w*max(0,w*(vlo-(ao*t_+v)))^2/(2*alo())": QE
                            ),
                          "w=1": andL('L=="t<=epsilon()-(t_+tt)&(0<=t&t < max(0,w*(vlo-(ao*t_+v)))/alo()&hn=w*alo()/2*t^2+(ao*t_+v)*t|t>=max(0,w*(vlo-(ao*t_+v)))/alo()&hn=vlo*t-w*max(0,w*(vlo-(ao*t_+v)))^2/(2*alo()))") ; orL('L=="0<=t&t < max(0,w*(vlo-(ao*t_+v)))/alo()&hn=w*alo()/2*t^2+(ao*t_+v)*t|t>=max(0,w*(vlo-(ao*t_+v)))/alo()&hn=vlo*t-w*max(0,w*(vlo-(ao*t_+v)))^2/(2*alo())") ; <(
                              "0<=t&t < max(0,w*(vlo-(ao*t_+v)))/alo()&hn=w*alo()/2*t^2+(ao*t_+v)*t": QE,
                              "t>=max(0,w*(vlo-(ao*t_+v)))/alo()&hn=vlo*t-w*max(0,w*(vlo-(ao*t_+v)))^2/(2*alo())": QE
                            )
                        )
                    ),
                  "t+t_>=Tlo(v,w,vlo)": allL("vlo*(t+t_)-w*maxI(v,w,vlo)^2/(2*alo())", 'L=="\forall hn (0<=t+t_&t+t_ < Tlo(v,w,vlo)&hn=w*alo()/2*(t+t_)^2+v*(t+t_)|t+t_>=Tlo(v,w,vlo)&hn=vlo*(t+t_)-w*maxI(v,w,vlo)^2/(2*alo())->abs(r-rv*(t+t_))>rp()|w*(hn-h)>hp())") ; simplify('L=="0<=t+t_&t+t_ < Tlo(v,w,vlo)&vlo*(t+t_)-w*maxI(v,w,vlo)^2/(2*alo())=w*alo()/2*(t+t_)^2+v*(t+t_)|t+t_>=Tlo(v,w,vlo)&vlo*(t+t_)-w*maxI(v,w,vlo)^2/(2*alo())=vlo*(t+t_)-w*maxI(v,w,vlo)^2/(2*alo())->abs(r-rv*(t+t_))>rp()|w*(vlo*(t+t_)-w*maxI(v,w,vlo)^2/(2*alo())-h)>hp()") ; orL('L=="abs(r-rv*(t+t_))>rp()|w*(vlo*(t+t_)-w*maxI(v,w,vlo)^2/(2*alo())-h)>hp()") ; <(
                      "abs(r-rv*(t+t_))>rp()": QE,
                      "w*(vlo*(t+t_)-w*maxI(v,w,vlo)^2/(2*alo())-h)>hp()": expandAllDefs ; implyR('R=="t<=epsilon()-(t_+tt)&(0<=t&t < max(0,w*(vlo-(ao*t_+v)))/alo()&hn=w*alo()/2*t^2+(ao*t_+v)*t|t>=max(0,w*(vlo-(ao*t_+v)))/alo()&hn=vlo*t-w*max(0,w*(vlo-(ao*t_+v)))^2/(2*alo()))->abs((-rv)*t_+r-rv*t)>rp()|w*(hn-(-(ao*(t_^2/2)+v*t_)+h))>hp()") ; andL('L=="t<=epsilon()-(t_+tt)&(0<=t&t < max(0,w*(vlo-(ao*t_+v)))/alo()&hn=w*alo()/2*t^2+(ao*t_+v)*t|t>=max(0,w*(vlo-(ao*t_+v)))/alo()&hn=vlo*t-w*max(0,w*(vlo-(ao*t_+v)))^2/(2*alo()))") ; orL('L=="w=(-1)|w=1") ; <(
                          "w=(-1)": orL('L=="0<=t&t < max(0,w*(vlo-(ao*t_+v)))/alo()&hn=w*alo()/2*t^2+(ao*t_+v)*t|t>=max(0,w*(vlo-(ao*t_+v)))/alo()&hn=vlo*t-w*max(0,w*(vlo-(ao*t_+v)))^2/(2*alo())") ; <(
                              "0<=t&t < max(0,w*(vlo-(ao*t_+v)))/alo()&hn=w*alo()/2*t^2+(ao*t_+v)*t": QE,
                              "t>=max(0,w*(vlo-(ao*t_+v)))/alo()&hn=vlo*t-w*max(0,w*(vlo-(ao*t_+v)))^2/(2*alo())": QE
                            ),
                          "w=1": orL('L=="0<=t&t < max(0,w*(vlo-(ao*t_+v)))/alo()&hn=w*alo()/2*t^2+(ao*t_+v)*t|t>=max(0,w*(vlo-(ao*t_+v)))/alo()&hn=vlo*t-w*max(0,w*(vlo-(ao*t_+v)))^2/(2*alo())") ; <(
                              "0<=t&t < max(0,w*(vlo-(ao*t_+v)))/alo()&hn=w*alo()/2*t^2+(ao*t_+v)*t": QE,
                              "t>=max(0,w*(vlo-(ao*t_+v)))/alo()&hn=vlo*t-w*max(0,w*(vlo-(ao*t_+v)))^2/(2*alo())": QE
                            )
                        )
                    )
                ),
              "Show": hideL('L=="\forall hn (t+t_<=epsilon()-tt&(0<=t+t_&t+t_ < Tlo(v,w,vlo)&hn=w*alo()/2*(t+t_)^2+v*(t+t_)|t+t_>=Tlo(v,w,vlo)&hn=vlo*(t+t_)-w*maxI(v,w,vlo)^2/(2*alo()))->abs(r-rv*(t+t_))>rp()|w*(hn-h)>hp())") ; orL('L=="w=wUp()|w=wLo()") ; <(
                  "w=wUp()": QE,
                  "w=wLo()": QE
