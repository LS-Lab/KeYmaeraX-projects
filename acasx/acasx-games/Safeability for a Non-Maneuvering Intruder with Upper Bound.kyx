                                        hideL('L=="\forall hn (0<=t&t < Tlo(w*max(w*vup,w*v),-w,vexUp)&hn=(-w)*alo()/2*t^2+w*max(w*vup,w*v)*t|t>=Tlo(w*max(w*vup,w*v),-w,vexUp)&hn=vexUp*t-(-w)*maxI(w*max(w*vup,w*v),-w,vexUp)^2/(2*alo())->abs(r-rv*(epsilon()-tt)-rv*t)>rp()|(-w)*(hn-(h-(w*max(w*vup,w*v)*(epsilon()-tt)-w*maxI(v,w,vup)^2/(2*aup()))))>hp())");
