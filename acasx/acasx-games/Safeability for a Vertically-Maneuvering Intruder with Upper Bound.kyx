Theorem "ACAS X Hybrid Games/Safeability Vertical inMotion".

ProgramVariables
  /* horizontal */
  Real r;    /* relative distance in ft; xi - xo */
             /* x = r */
  Real rv;   /* relative speed -(vi - vo) */
  
  /* vertical */
  Real h;    /* relative altitude in ft */
             /* if negative: the intruder is lower than the ownship */
             /* h := hi - ho */
             /* z = h */
  Real v;    /* vertical velocity of ownship */
  Real vlo;  /* target minimum vertical velocity */
  Real vup;  /* target maximum vertical velocity */
  Real w;    /* velocity bound */
  Real ao;   /* ownship acceleration */
  Real ai;   /* intruder vertical acceleration */
  Real co;   /* ownship's estimation of worst-case intruder behavior */
  
  Real tt;   /* clock */
End.

Problem
  loopInv(r,h,v,w,vlo,vup,rv,tt)&co=c()
  ->\exists ao ((-amax()<=ao&ao<=amax())&[{ai:=*;?-co < ai&ai < co;inMotion{|^@|};}*]loopInv(r,h,v,w,vlo,vup,rv,tt))
End.

Tactic "ACAS X Hybrid Games/Proof Safeability Vertical inMotion"
implyR('R=="loopInv(r,h,v,w,vlo,vup,rv,tt)&co=c()->\exists ao ((-amax()<=ao&ao<=amax())&[{ai:=*;?-co < ai&ai < co;inMotion{|^@|};}*]loopInv(r,h,v,w,vlo,vup,rv,tt))");
existsR("0", 'R=="\exists ao ((-amax()<=ao&ao<=amax())&[{ai:=*;?-co < ai&ai < co;inMotion{|^@|};}*]loopInv(r,h,v,w,vlo,vup,rv,tt))");
andR('R=="(-amax()<=ao&ao<=amax())&[{ai:=*;?-co < ai&ai < co;inMotion{|^@|};}*]loopInv(r,h,v,w,vlo,vup,rv,tt)"); <(
  "-amax()<=ao&ao<=amax()":
    expand "loopInv";
    expand "init";
    QE,
  "[{ai:=*;?-co < ai&ai < co;inMotion{|^@|};}*]loopInv(r,h,v,w,vlo,vup,rv,tt)":
    loopRule("loopInv(r,h,v,w,vlo,vup,rv,tt)&co=c()&ao=0", 'R=="[{ai:=*;?-co < ai&ai < co;inMotion{|^@|};}*]loopInv(r,h,v,w,vlo,vup,rv,tt)"); <(
      "Init":
        simplify('R=="loopInv(r,h,v,w,vlo,vup,rv,tt)&co=c()&ao=0");
        QE,
      "Step":
        unfold;
        expand "inMotion";
        cut("w*v < w*vlo|w*vlo<=w*v&w*v<=w*vup|w*vup < w*v"); <(
          "Use":
            orL('L=="w*v < w*vlo|w*vlo<=w*v&w*v<=w*vup|w*vup < w*v"); <(
              "w*v < w*vlo":
                diffUnpackEvolDomain('R=="[{r'=-rv,h'=-v,v'=ao-ai,tt'=1&tt<=epsilon()&w*vlo<=w*v&w*v<=w*vup}](loopInv(r,h,v,w,vlo,vup,rv,tt)&co=c()&ao=0)");
                hideR('R=="[{r'=-rv,h'=-v,v'=ao-ai,tt'=1&tt<=epsilon()&w*vlo<=w*v&w*v<=w*vup}](loopInv(r,h,v,w,vlo,vup,rv,tt)&co=c()&ao=0)");
                QE,
              "w*vlo<=w*v&w*v<=w*vup|w*vup < w*v":
                orL('L=="w*vlo<=w*v&w*v<=w*vup|w*vup < w*v"); <(
                  "w*vlo<=w*v&w*v<=w*vup":
                    solve('R=="[{r'=-rv,h'=-v,v'=ao-ai,tt'=1&tt<=epsilon()&w*vlo<=w*v&w*v<=w*vup}](loopInv(r,h,v,w,vlo,vup,rv,tt)&co=c()&ao=0)");
                    unfold; <(
                      "loopInv((-rv)*t_+r,-((ao-ai)*(t_^2/2)+v*t_)+h,(ao-ai)*t_+v,w,vlo,vup,rv,t_+tt)":
                        expand "loopInv";
                        expand "init";
                        allL("t_", 'L=="\forall s_ (0<=s_&s_<=t_->s_+tt<=epsilon()&w*vlo<=w*((ao-ai)*s_+v)&w*((ao-ai)*s_+v)<=w*vup)");
                        simplify('L=="0<=t_&t_<=t_->t_+tt<=epsilon()&w*vlo<=w*((ao-ai)*t_+v)&w*((ao-ai)*t_+v)<=w*vup");
                        andL('L)*;
                        simplify('R=="0<=t_+tt&t_+tt<=epsilon()&(rp()>=0&hp()>0&rv>=0&alo()>0&aup()>alo()+2*c()&c()>0&amax()>=alo()+c()&(w=wUp()|w=wLo())&epsilon()>=0)&CimplSafe((-rv)*t_+r,-((ao-ai)*(t_^2/2)+v*t_)+h,(ao-ai)*t_+v,w,vlo,vup,rv,epsilon()-(t_+tt))");
                        andR('R=="0<=t_+tt&CimplSafe((-rv)*t_+r,-((ao-ai)*(t_^2/2)+v*t_)+h,(ao-ai)*t_+v,w,vlo,vup,rv,epsilon()-(t_+tt))"); <(
                          "0<=t_+tt":
                            QE,
                          "CimplSafe((-rv)*t_+r,-((ao-ai)*(t_^2/2)+v*t_)+h,(ao-ai)*t_+v,w,vlo,vup,rv,epsilon()-(t_+tt))":
                            expand "CimplSafe";
                            andL('L=="w*vlo<=w*vup&(LimplSafe(r,h,v,w,vlo,rv,epsilon()-tt)|UimplSafe(r,h,v,w,vup,rv,epsilon()-tt))");
                            simplify('R=="w*vlo<=w*vup&(LimplSafe((-rv)*t_+r,-((ao-ai)*(t_^2/2)+v*t_)+h,(ao-ai)*t_+v,w,vlo,rv,epsilon()-(t_+tt))|UimplSafe((-rv)*t_+r,-((ao-ai)*(t_^2/2)+v*t_)+h,(ao-ai)*t_+v,w,vup,rv,epsilon()-(t_+tt)))");
                            orR('R=="LimplSafe((-rv)*t_+r,-((ao-ai)*(t_^2/2)+v*t_)+h,(ao-ai)*t_+v,w,vlo,rv,epsilon()-(t_+tt))|UimplSafe((-rv)*t_+r,-((ao-ai)*(t_^2/2)+v*t_)+h,(ao-ai)*t_+v,w,vup,rv,epsilon()-(t_+tt))");
                            orL('L=="LimplSafe(r,h,v,w,vlo,rv,epsilon()-tt)|UimplSafe(r,h,v,w,vup,rv,epsilon()-tt)"); <(
                              "LimplSafe(r,h,v,w,vlo,rv,epsilon()-tt)":
                                hideR('R=="UimplSafe((-rv)*t_+r,-((ao-ai)*(t_^2/2)+v*t_)+h,(ao-ai)*t_+v,w,vup,rv,epsilon()-(t_+tt))");
                                expand "LimplSafe";
                                andL('L=="LimplEp(r,h,v,w,vlo,rv,epsilon()-tt)&\forall hexL \forall vexL (0<=epsilon()-tt&epsilon()-tt < Tlo(v,w,vlo)&hexL=w*alo()/2*(epsilon()-tt)^2+v*(epsilon()-tt)&vexL=w*alo()*(epsilon()-tt)+v|epsilon()-tt>=Tlo(v,w,vlo)&hexL=vlo*(epsilon()-tt)-w*maxI(v,w,vlo)^2/(2*alo())&vexL=vlo->\exists vexLo Limpl(w,vexLo,vexL,r-rv*(epsilon()-tt),rv,h-hexL))");
                                andR('R=="LimplEp((-rv)*t_+r,-((ao-ai)*(t_^2/2)+v*t_)+h,(ao-ai)*t_+v,w,vlo,rv,epsilon()-(t_+tt))&\forall hexL \forall vexL (0<=epsilon()-(t_+tt)&epsilon()-(t_+tt) < Tlo((ao-ai)*t_+v,w,vlo)&hexL=w*alo()/2*(epsilon()-(t_+tt))^2+((ao-ai)*t_+v)*(epsilon()-(t_+tt))&vexL=w*alo()*(epsilon()-(t_+tt))+((ao-ai)*t_+v)|epsilon()-(t_+tt)>=Tlo((ao-ai)*t_+v,w,vlo)&hexL=vlo*(epsilon()-(t_+tt))-w*maxI((ao-ai)*t_+v,w,vlo)^2/(2*alo())&vexL=vlo->\exists vexLo Limpl(w,vexLo,vexL,(-rv)*t_+r-rv*(epsilon()-(t_+tt)),rv,-((ao-ai)*(t_^2/2)+v*t_)+h-hexL))"); <(
                                  "LimplEp((-rv)*t_+r,-((ao-ai)*(t_^2/2)+v*t_)+h,(ao-ai)*t_+v,w,vlo,rv,epsilon()-(t_+tt))":
                                    hideL('L=="\forall hexL \forall vexL (0<=epsilon()-tt&epsilon()-tt < Tlo(v,w,vlo)&hexL=w*alo()/2*(epsilon()-tt)^2+v*(epsilon()-tt)&vexL=w*alo()*(epsilon()-tt)+v|epsilon()-tt>=Tlo(v,w,vlo)&hexL=vlo*(epsilon()-tt)-w*maxI(v,w,vlo)^2/(2*alo())&vexL=vlo->\exists vexLo Limpl(w,vexLo,vexL,r-rv*(epsilon()-tt),rv,h-hexL))");
                                    expand "LimplEp";
                                    allR('R=="\forall t \forall hn (t<=epsilon()-(t_+tt)&Alo((ao-ai)*t_+v,w,vlo,hn,t)->abs((-rv)*t_+r-rv*t)>rp()|w*(hn-(-((ao-ai)*(t_^2/2)+v*t_)+h))>hp())");
                                    allR('R=="\forall hn (t<=epsilon()-(t_+tt)&Alo((ao-ai)*t_+v,w,vlo,hn,t)->abs((-rv)*t_+r-rv*t)>rp()|w*(hn-(-((ao-ai)*(t_^2/2)+v*t_)+h))>hp())");
