                              "\forall hexL \forall vexL (0<=epsilon()-(t_+tt)&epsilon()-(t_+tt) < Tlo((ao-ai)*t_+v,w,vlo)&hexL=w*alo()/2*(epsilon()-(t_+tt))^2+((ao-ai)*t_+v)*(epsilon()-(t_+tt))&vexL=w*alo()*(epsilon()-(t_+tt))+((ao-ai)*t_+v)|epsilon()-(t_+tt)>=Tlo((ao-ai)*t_+v,w,vlo)&hexL=vlo*(epsilon()-(t_+tt))-w*maxI((ao-ai)*t_+v,w,vlo)^2/(2*alo())&vexL=vlo->\exists vexLo Limpl(w,vexLo,vexL,(-rv)*t_+r-rv*(epsilon()-(t_+tt)),rv,-((ao-ai)*(t_^2/2)+v*t_)+h-hexL))":
                                allR('R=="\forall hexL \forall vexL (0<=epsilon()-(t_+tt)&epsilon()-(t_+tt) < Tlo((ao-ai)*t_+v,w,vlo)&hexL=w*alo()/2*(epsilon()-(t_+tt))^2+((ao-ai)*t_+v)*(epsilon()-(t_+tt))&vexL=w*alo()*(epsilon()-(t_+tt))+((ao-ai)*t_+v)|epsilon()-(t_+tt)>=Tlo((ao-ai)*t_+v,w,vlo)&hexL=vlo*(epsilon()-(t_+tt))-w*maxI((ao-ai)*t_+v,w,vlo)^2/(2*alo())&vexL=vlo->\exists vexLo Limpl(w,vexLo,vexL,(-rv)*t_+r-rv*(epsilon()-(t_+tt)),rv,-((ao-ai)*(t_^2/2)+v*t_)+h-hexL))") ; allR('R=="\forall vexL (0<=epsilon()-(t_+tt)&epsilon()-(t_+tt) < Tlo((ao-ai)*t_+v,w,vlo)&hexL=w*alo()/2*(epsilon()-(t_+tt))^2+((ao-ai)*t_+v)*(epsilon()-(t_+tt))&vexL=w*alo()*(epsilon()-(t_+tt))+((ao-ai)*t_+v)|epsilon()-(t_+tt)>=Tlo((ao-ai)*t_+v,w,vlo)&hexL=vlo*(epsilon()-(t_+tt))-w*maxI((ao-ai)*t_+v,w,vlo)^2/(2*alo())&vexL=vlo->\exists vexLo Limpl(w,vexLo,vexL,(-rv)*t_+r-rv*(epsilon()-(t_+tt)),rv,-((ao-ai)*(t_^2/2)+v*t_)+h-hexL))") ; expand "init" ; allL2R('L=="co=c()") ; andL('L)* ; hideL('L=="LimplEp(r,h,v,w,vlo,rv,epsilon()-tt)") ; cut("0<=epsilon()-tt&epsilon()-tt < Tlo(v,w,vlo)|epsilon()-tt>=Tlo(v,w,vlo)"); <(
