Theorem "ACAS X Hybrid Games/Safeability Vertical inUpEvent".

ProgramVariables
  /* horizontal */
  Real r;    /* relative distance in ft; xi - xo */
             /* x = r */
  Real rv;   /* relative speed -(vi - vo) */
  
  /* vertical */
  Real h;    /* relative altitude in ft */
             /* if negative: the intruder is lower than the ownship */
             /* h := hi - ho */
             /* z = h */
  Real v;    /* vertical velocity of ownship */
  Real vlo;  /* target minimum vertical velocity */
  Real vup;  /* target maximum vertical velocity */
  Real w;    /* velocity bound */
  Real ao;   /* ownship acceleration */
  Real ai;   /* intruder vertical acceleration */
  Real co;   /* ownship's estimation of worst-case intruder behavior */
  
  Real tt;   /* clock */
End.

Problem
  loopInv(r,h,v,w,vlo,vup,rv,tt)&co=c()
  ->\exists ao ((-amax()<=ao&ao<=amax())&[{ai:=*;?-co < ai&ai < co;inUpEvent{|^@|};}*]loopInv(r,h,v,w,vlo,vup,rv,tt))
End.

Tactic "ACAS X Hybrid Games/Proof Safeability Vertical inUpEvent"
implyR('R=="loopInv(r,h,v,w,vlo,vup,rv,tt)&co=c()->\exists ao ((-amax()<=ao&ao<=amax())&[{ai:=*;?-co < ai&ai < co;inUpEvent{|^@|};}*]loopInv(r,h,v,w,vlo,vup,rv,tt))");
existsR("w*(-c())", 'R=="\exists ao ((-amax()<=ao&ao<=amax())&[{ai:=*;?-co < ai&ai < co;inUpEvent{|^@|};}*]loopInv(r,h,v,w,vlo,vup,rv,tt))");
andR('R=="(-amax()<=ao&ao<=amax())&[{ai:=*;?-co < ai&ai < co;inUpEvent{|^@|};}*]loopInv(r,h,v,w,vlo,vup,rv,tt)"); <(
  "-amax()<=ao&ao<=amax()":
    andL('L=="loopInv(r,h,v,w,vlo,vup,rv,tt)&co=c()");
    expand "loopInv";
    expand "init";
    andL('L)*;
    hideL('L=="CimplSafe(r,h,v,w,vlo,vup,rv,epsilon()-tt)");
    QE,
  "[{ai:=*;?-co < ai&ai < co;inUpEvent{|^@|};}*]loopInv(r,h,v,w,vlo,vup,rv,tt)":
    andL('L=="loopInv(r,h,v,w,vlo,vup,rv,tt)&co=c()");
    expand "inUpEvent";
    loopRule("loopInv(r,h,v,w,vlo,vup,rv,tt)&ao=w*(-c())&co=c()", 'R=="[{ai:=*;?-co < ai&ai < co;{r'=-rv,h'=-v,v'=ao-ai,tt'=1&tt<=epsilon()&w*vup<=w*v}}*]loopInv(r,h,v,w,vlo,vup,rv,tt)"); <(
      "Init":
        fullSimplify;
        closeT,
      "Step":
        unfold;
        cut("w*vup>w*v|w*vup<=w*v"); <(
          "Use":
            orL('L=="w*vup>w*v|w*vup<=w*v"); <(
              "w*vup>w*v":
                diffUnpackEvolDomain('R=="[{r'=-rv,h'=-v,v'=ao-ai,tt'=1&tt<=epsilon()&w*vup<=w*v}](loopInv(r,h,v,w,vlo,vup,rv,tt)&ao=w*(-c())&co=c())");
                hideR('R=="[{r'=-rv,h'=-v,v'=ao-ai,tt'=1&tt<=epsilon()&w*vup<=w*v}](loopInv(r,h,v,w,vlo,vup,rv,tt)&ao=w*(-c())&co=c())");
                QE using "-co < ai :: ai < co :: ao=w*(-c()) :: co=c() :: w*vup>w*v :: tt<=epsilon()&w*vup<=w*v :: nil",
              "w*vup<=w*v":
                solve('R=="[{r'=-rv,h'=-v,v'=ao-ai,tt'=1&tt<=epsilon()&w*vup<=w*v}](loopInv(r,h,v,w,vlo,vup,rv,tt)&ao=w*(-c())&co=c())");
                unfold; <(
                  "loopInv((-rv)*t_+r,-((ao-ai)*(t_^2/2)+v*t_)+h,(ao-ai)*t_+v,w,vlo,vup,rv,t_+tt)":
                    allL("t_", 'L=="\forall s_ (0<=s_&s_<=t_->s_+tt<=epsilon()&w*vup<=w*((ao-ai)*s_+v))");
                    expand "loopInv";
