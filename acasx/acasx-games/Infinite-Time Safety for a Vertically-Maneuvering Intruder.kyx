      "Show [a]Q":
        composeb('R=="[{ao:=*;?-amax()<=ao&ao<=amax();}^@{ai:=*;?-c() < ai&ai < c();motion{|^@|};}*]loopInv(r,h,v,w,vlo,rv)");
        chaseAt('R=="[{ao:=*;?-amax()<=ao&ao<=amax();}^@][{ai:=*;?-c() < ai&ai < c();motion{|^@|};}*]loopInv(r,h,v,w,vlo,rv)");
        cut("w*v>=w*vlo|w*v < w*vlo"); <(
          "Use":
            orL('L=="w*v>=w*vlo|w*v < w*vlo"); <(
