                existsR("w*c()", 'R=="\exists ao ((-amax()<=ao&ao<=amax())&[{ai:=*;?-c() < ai&ai < c();{r'=-rv,h'=-v,v'=ao-ai}}*]loopInv(r,h,v,w,vlo,rv))");
