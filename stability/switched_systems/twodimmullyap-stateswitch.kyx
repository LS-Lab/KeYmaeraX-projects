                            ODE('R=="[{x1'=-x1/8-x2,x2'=2*x1-x2/8,t'=1&(x1>=0&2*x1^2+x2^2 < k)&x1^2+x2^2!=eps^2}]x1^2+x2^2 < eps^2")
                        ),
                      "Show":
                        dW('R=="[{x1'=-x1/8-x2,x2'=2*x1-x2/8,t'=1&x1>=0&lyap1(x1,x2) < k}]normsq(x1,x2)!=eps^2");
                        unfold;
