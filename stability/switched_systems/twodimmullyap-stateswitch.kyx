                        allL('L=="\forall x2 (normsq(x1,x2)=eps^2->(x1>=0->lyap1(x1,x2)>=k)&(x1<=0->lyap2(x1,x2)>=k))");
                        expandAllDefs;
                        QE
                    ),
                  "Show":
                    hideL('L=="\forall x1 \forall x2 (normsq(x1,x2)=eps^2->(x1>=0->lyap1(x1,x2)>=k)&(x1<=0->lyap2(x1,x2)>=k))");
